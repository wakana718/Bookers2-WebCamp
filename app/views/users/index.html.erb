<div class="container">
  <div class="row">
      <div class="col-xs-3" >

<%= render 'users/list', user: @user %>


<%= render 'books/list2', book: @book %>

 <div class="row">
<div class="col-xs-3">
<h1>Users</h1>
　<table class="table table-hover">
　  <thead>
　    <th>image</th>
　    <th>name</th>
　    <th colspan="3"></th>
　  </thead>
　  <tbody>
　    <% @users.each do |user| %>
　    <tr>
       <td><%= attachment_image_tag @user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no-image-mini.jpg" %></td>
       <td><%= user.name %></td>

       <td><%= link_to "Show", user_path(user) %></td>
　    </tr>
　    <% end %>
　  </tbody>
　</table>
　</div>
</div>